Tasks = ["MiniGrid-MyUnlock1-v0", "MiniGrid-UpperTunel-v0"]

def make_env(env_name, env_kwargs={}, seed=None):
    def _make_env():
        env = gym.make(env_name, **env_kwargs)
        if hasattr(env, 'seed'):
            env.seed(seed)
        return env
    return _make_env

class TwoDifferentTask:
    def __init__(self):
        self.kernel = make_env(Tasks[0]) 

    def reset_task(self, task: int):
        self.kernel = make_env(Tasks[0])
    
    def step(self, action):
        return self.kernel.step(action)

    def reset(self):
        return self.kernel.reset()